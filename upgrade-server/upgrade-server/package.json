{
  "name": "remote-upgrade-server",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "tsc && export PORT=8000 && node ./bin/www",
    "windows_start": "tsc && set PORT=8000 && node ./bin/www",
    "linux_start": "tsc && export PORT=8000 && nohup node ./bin/www &",
    "linux_restart": "ps -ef | grep \"/bin/www\" |grep -v grep |  awk '{print $2}' | xargs kill -9 && npm run linux_start",
    "linux_stop": "ps -ef | grep \"/bin/www\" |grep -v grep |  awk '{print $2}' | xargs kill -9",
    "build": "tsc"
  },
  "dependencies": {
    "@types/bluebird": "^3.5.24",
    "@types/cookie-parser": "^1.4.1",
    "@types/dateformat": "^1.0.1",
    "@types/express": "^4.16.0",
    "@types/formidable": "^1.0.31",
    "@types/http-errors": "^1.6.1",
    "@types/morgan": "^1.7.35",
    "@types/mysql": "^2.15.5",
    "@types/redis": "^2.8.8",
    "@types/request": "^2.47.1",
    "@types/superagent": "^3.8.5",
    "@types/validator": "^10.11.0",
    "@types/xml2js": "^0.4.3",
    "bluebird": "^3.5.3",
    "child_process": "^1.0.2",
    "cookie-parser": "~1.4.3",
    "dateformat": "^3.0.3",
    "debug": "~2.6.9",
    "express": "~4.16.0",
    "express-session": "^1.15.6",
    "formidable": "^1.2.1",
    "http-errors": "~1.6.2",
    "iconv-lite": "^0.4.23",
    "log4js": "^3.0.6",
    "moment": "^2.22.2",
    "morgan": "~1.9.0",
    "mysql": "^2.16.0",
    "node-cmd": "^3.0.0",
    "node-xlsx": "^0.12.1",
    "redis": "^2.8.0",
    "request": "^2.87.0",
    "string-format": "^2.0.0",
